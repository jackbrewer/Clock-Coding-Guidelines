!!! 5
html(lang='en')
	head
		block head
			meta(charset='utf-8')
			meta(name='description', content='')
			meta(name='keywords', content='')
		
			//- SITE INFORMATION
			title Clock Coding Guidelines

		block styles
			//- CSS
			link(rel='stylesheet', href='/css/main/index.css')
			link(rel='stylesheet', href='/js/google-code-prettify/prettify.css')

		script
			document.getElementsByTagName('html')[0].setAttribute('class', 'js');

	body
		#wrapper
			#container
				block header
					include includes/header

				block nav
					include includes/nav
				
				block content
					#main-content
						!= body

				block footer
					include includes/footer

		block script
			script(src='//ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js')
			script
				if (typeof jQuery === 'undefined') {
					document.write('<script src="/js/jquery-1.7.1.min.js" />');
				}
			script(src='//ajax.googleapis.com/ajax/libs/jqueryui/1.8.16/jquery-ui.min.js')
			script
				if (typeof jQuery.ui === 'undefined') {
					document.write('<script src="/js/jquery-ui-1.8.16.min.js" />');
				}
			script(src='/js/google-code-prettify/prettify.js')
			script(src='/js/yScroll.js')
			script(src='/js/script.js')